<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="'App\Entity'\Oeuvres" table="oeuvres">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idCategories" type="integer" column="id_categories" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dimensions" type="string" column="dimensions" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="techniques" type="text" column="techniques" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="photo" type="string" column="photo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="integer" column="prix" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateImportAt" type="datetime" column="date_import_at" nullable="false"/>
    <field name="statut" type="boolean" column="statut" nullable="true"/>
  </entity>
</doctrine-mapping>
